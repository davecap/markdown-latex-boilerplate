\newcommand\maketitle{\begin{titlepage}%
  \let\footnotesize\small
  \let\footnoterule\relax
  \let \footnote \thanks
  \null\vfil
  \vskip 60\p@
  \begin{center}%
    {\LARGE \@title \par}%
    \vskip 3em%
    {\large
     \lineskip .75em%
      \begin{tabular}[t]{c}%
        by
      \end{tabular}\par}%
      \vskip 1.0em%
    {\large
     \lineskip .75em%
      \begin{tabular}[t]{c}%
        \@author
      \end{tabular}\par}%
      \vskip 1.5em%
  \end{center}\par
  \vskip 30\p@

  \begin{raggedright}%
  A dissertation submitted to the Graduate Faculty in Earth and Environmental
  Sciences in partial fulfillment of the requirements for the degree of
  Doctor of Philosophy, The City University of New York.
  \end{raggedright}\par
  \begin{center}%
    \number\year
  \end{center}\par
  \end{titlepage}
}

